{
  "path-intellisense.mappings": {
    "@pages": "${workspaceFolder}/pages",
    "@organisms": "${workspaceFolder}/components/organisms",
    "@molecules": "${workspaceFolder}/components/molecules",
    "@atoms": "${workspaceFolder}/components/atoms",
    "@icons": "${workspaceFolder}/components/icons",
    "@components": "${workspaceFolder}/components",
    "@data": "${workspaceFolder}/data",
    "@utils": "${workspaceFolder}/utils",
    "@context": "${workspaceFolder}/context",
    "@images": "${workspaceFolder}/public/assets/images",
    "@fonts": "${workspaceFolder}/public/assets/fonts",
    "@assets": "${workspaceFolder}/public/assets",
    "@styles": "${workspaceFolder}/styles",
    "@": "${workspaceFolder}"
  },
  "path-intellisense.autoTriggerNextSuggestion": true,
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "workbench.colorCustomizations": {
    "sash.hoverBorder": "#ffc78a",
    "statusBar.background": "#ffae57",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#ff9524",
    "statusBarItem.remoteBackground": "#ffae57",
    "statusBarItem.remoteForeground": "#15202b"
  },
  "peacock.color": "#FFAE57",
  "postcss.validate": false 
}
